{
  "chart_type": "seasonal",
  "colors": [
    "#FF6B6B",
    "#4ECDC4",
    "#45B7D1"
  ],
  "layout": {
    "width": "1200px",
    "height": "600px",
    "theme": "white"
  },
  "options": {
    "title": {
      "text": "季节性分析",
      "subtitle": "",
      "left": "center"
    },
    "tooltip": {
      "trigger": "axis",
      "axis_pointer_type": "cross"
    },
    "legend": {
      "type": "scroll",
      "orient": "horizontal",
      "top": "5%",
      "left": "center"
    },
    "grid": {
      "left": "3%",
      "right": "4%",
      "bottom": "3%",
      "containLabel": true
    },
    "xAxis": {
      "type": "category",
      "boundary_gap": false
    },
    "yAxis": {
      "type": "value"
    },
    "dataZoom": [
      {
        "type": "inside",
        "start": 0,
        "end": 100
      },
      {
        "type": "slider",
        "start": 0,
        "end": 100
      }
    ]
  }
}